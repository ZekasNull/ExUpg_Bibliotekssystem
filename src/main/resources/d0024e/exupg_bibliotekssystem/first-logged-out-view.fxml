<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.FirstLoggedOutViewController">
   <children>
      <Button fx:id="GoToLoginViewButton" layoutX="524.0" layoutY="14.0" mnemonicParsing="false" onAction="#onUserGoToLoginViewButtonClick" text="Logga in" />
      <Label layoutX="182.0" layoutY="39.0" text="Skriv in vad du vill söka på:">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="searchtermBoxContents" layoutX="182.0" layoutY="77.0" prefHeight="25.0" prefWidth="237.0" />
      <Button fx:id="NotLoggedInSearchButton" layoutX="419.0" layoutY="77.0" mnemonicParsing="false" onAction="#onNotLoggedInSearchButtonClick" text="Sök" />
      <Label layoutX="182.0" layoutY="109.0" text="Objekttyp" />
      <SplitMenuButton fx:id="objektTypFlerVal" layoutX="182.0" layoutY="133.0" mnemonicParsing="false" text="Välj objekt">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#handleBokOption" text="Bok" />
          <MenuItem mnemonicParsing="false" onAction="#handleFilmOption" text="Film" />
            <MenuItem mnemonicParsing="false" onAction="#handleTidskriftOption" text="Tidskrift" />
        </items>
      </SplitMenuButton>
      <TableView fx:id="notLoggedInBookSearchTable" layoutX="44.0" layoutY="170.0" prefHeight="200.0" prefWidth="526.0" visible="false">
        <columns>
          <TableColumn fx:id="titleColumn" prefWidth="150.0" text="Titel" />
          <TableColumn fx:id="isbnColumn" prefWidth="107.0" text="ISBN-13" />
            <TableColumn fx:id="subjectColumn" prefWidth="143.0" text="Ämnesord" />
            <TableColumn fx:id="authorNameColumn" prefWidth="125.0" text="Författare namn" />
        </columns>
      </TableView>
      <TableView fx:id="notLoggedInFilmSearchTable" layoutX="30.0" layoutY="170.0" prefHeight="200.0" prefWidth="552.0" visible="false">
        <columns>
          <TableColumn fx:id="filmTitleColumn" prefWidth="75.0" text="Titel" />
          <TableColumn fx:id="productionCountryColumn" prefWidth="75.0" text="Prod. land" />
            <TableColumn fx:id="ageLimitColumn" prefWidth="91.0" text="Åldersgräns" />
            <TableColumn fx:id="directorsColumn" prefWidth="109.0" text="Regissör" />
            <TableColumn fx:id="actorsColumn" prefWidth="124.0" text="Skådespelare" />
            <TableColumn fx:id="genreColumn" prefWidth="76.0" text="Genre" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
